{
  "version": 2,
  "builds": [
    { "src": "api.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/fluxus", "dest": "api.py" },
    { "src": "/api/paste", "dest": "api.py" },
    { "src": "/api/socialwolvez", "dest": "api.py" },
    { "src": "/api/mboost", "dest": "api.py" },
    { "src": "/api/mediafire", "dest": "api.py" },
    { "src": "/supported", "dest": "api.py" },
    { "src": "/", "dest": "/static/index.html" },
    { "src": "/(.*)", "dest": "/static/index.html" }
  ]
}
