{
  "version": 2,
  "builds": [
    { "src": "api.py", "use": "@vercel/python" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/public/home.html" },
    { "src": "/api/(.*)", "dest": "/api.py" },
    { "src": "/supported", "dest": "/api.py" }
  ]
}
