{
  "version": 2,
  "builds": [
    { "src": "api.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/bypass", "dest": "api.py" },
    { "src": "/supported", "dest": "api.py" },
    { "src": "/static/(.*)", "dest": "/public/$1" },
    { "src": "/", "dest": "/public/index.html" }
  ]
}
